{
    "files.exclude": {
        "**/.git": true,
        "**/.cache": true,
    },
    "search.exclude": {
        ".cache": true,
        ".git": true,
        "llvm-project/build/**": true,
    },
    "files.associations": {
        "*.logcat": "logcat",
        "*.inc": "cpp"
    },

    // Note: Controlls verbosity of C/C++ output log. Set to Debug for full logging
    "C_Cpp.loggingLevel": "Warning",
    "cSpell.words": [
        "Vectorize",
        "Vectorizer"
    ],
}